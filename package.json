{
  "name": "voice-bridge-translator",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "NODE_ENV=production tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "tsx api/server.ts",
    "server:prod": "NODE_ENV=production tsx api/server.ts",
    "start": "concurrently \"pnpm run server\" \"pnpm run dev\"",
    "start:prod": "NODE_ENV=production node dist-server/server.js",
    "migrate": "tsx scripts/migrate.ts",
    "quick-start": "node setup-env.js && pnpm run start"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.0.0",
    "@neondatabase/serverless": "^0.9.0",
    "ably": "^2.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
